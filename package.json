{
  "name": "data-entry",
  "version": "1.0.0",
  "description": "data entry and image viewer",
  "main": "./src/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build":{
    "productName":"Data Entry Prototype",
    "extraFiles": ["config/"]
  },
  "keywords": [
    "data",
    "entry"
  ],
  "author": "nbriones",
  "license": "ISC",
  "devDependencies": {
    "@types/jquery": "^3.3.17",
    "electron": "latest",
    "electron-build": "latest",
    "electron-builder": "latest"
  },
  "win": {
    "target": "NSIS"
  },
  "dependencies": {
    "bootstrap": "^4.1.2",
    "electron-packager": "^12.2.0",
    "jquery": "^3.3.1",
    "jquery-ui": "file:./lib/jquery-ui"
  }
}
